{
    "compilerOptions": {
        "outDir": "./dist",
        "target": "ES2022",
        "module": "NodeNext",
        "moduleResolution": "nodenext",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "removeComments": true,
        "strictNullChecks": true
    },
    "include": ["app/**/*"]
}